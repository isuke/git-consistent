{
  "name": "git-consistent",
  "version": "1.0.4",
  "description": "Give consistency to your project's git logs.",
  "keywords": [
    "git",
    "commit",
    "commitizen"
  ],
  "homepage": "https://github.com/isuke/git-consistent",
  "repository": "git@github.com:isuke/git-consistent.git",
  "author": "isuke <isuke770@gmail.com>",
  "license": "MIT",
  "private": false,
  "preferGlobal": true,
  "engines": {
    "node": "<=12"
  },
  "bin": {
    "git-consistent": "./lib/index.js"
  },
  "scripts": {
    "deploy": "./deploy.sh",
    "lint": "eslint lib/**/*.js test/**/*.js",
    "fix": "eslint --fix lib/**/*.js test/**/*.js",
    "test": "ava --verbose"
  },
  "dependencies": {
    "commander": "4.1.1",
    "csv-parse": "4.11.1",
    "inquirer": "7.3.2",
    "js-yaml": "3.14.0",
    "lodash": "4.17.19",
    "node-emoji": "1.10.0",
    "prompt-sync": "4.2.0"
  },
  "devDependencies": {
    "ava": "3.10.1",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "outdent": "0.7.1",
    "prettier": "2.0.5",
    "sinon": "9.0.2"
  }
}
